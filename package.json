{
  "name": "xenteck-static-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run tailwind:build && npm run bundle && node ./scripts/build-static.mjs",
    "tailwind:build": "tailwindcss -i ./assets/css/tailwind.input.css -o ./assets/css/tailwind.css --minify",
    "tailwind:watch": "tailwindcss -i ./assets/css/tailwind.input.css -o ./assets/css/tailwind.css --watch",
    "bundle": "esbuild assets/js/visualizer.jsx --bundle --format=esm --outfile=assets/js/dist/visualizer.bundle.js --main-fields=module,main",
    "bundle:watch": "esbuild assets/js/visualizer.jsx --bundle --format=esm --outfile=assets/js/dist/visualizer.bundle.js --main-fields=module,main --watch"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.2.1"
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.4"
  }
}
